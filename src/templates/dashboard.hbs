<h4>Wellcome to dashboard view!</h4>
<div class="dashboard">
  <h4 class="text-primary"><i class="glyphicon glyphicon-dashboard"></i> Suggestions</h4>
  <div class="row">
    {{#suggestions}}
    <div class="col-sm-6 text-info">
      <div class="app-card">{{message}}</div>
    </div>
    {{/suggestions}}
  </div>
  <h4 class="text-primary"><i class="glyphicon glyphicon-bell"></i> Notifications</h4>
  <div class="row">
  {{#notifications}}
  <div class="col-sm-6">
    <div class="app-card notification">
      <div>
        <div class="glyphicon glyphicon-envelope text-success"></div>
      </div>
      <div>
        <div class="message">{{upper message}}</div>
        <div class="date">{{moment date 'YY-MM-DD HH:MM:ss'}}</div>
      </div>
    </div>
  </div>
  {{/notifications}}
  </div>
</div>